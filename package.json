{
	"name": "speak5languages",
	"version": "1.0.0",
	"description": "![project language](https://img.shields.io/badge/TypeScript-007ACC?style=for-the-badge&logo=typescript&logoColor=white) ![react](https://img.shields.io/badge/React-20232A?style=for-the-badge&logo=react&logoColor=61DAFB) ![nodejs](https://img.shields.io/badge/Node%20js-339933?style=for-the-badge&logo=nodedotjs&logoColor=white)",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev src/index.ts",
		"build": "npx tsc && node ./dist/main.js",
		"test": "npx tsc && jest ./dist",
		"prepare": "husky",
		"typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/infra/database/ormconfig.ts ./src/infra/database/migrations/default",
		"typeorm:migrate": "typeorm-ts-node-commonjs  migration:run -d ./src/infra/database/ormconfig.ts"
	},
	"lint-staged": {
		"*": [
			"biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
		]
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^22.3.0",
		"@types/uuid": "^10.0.0",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.10",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.19.2",
		"express-async-errors": "^3.1.1",
		"pg": "^8.13.0",
		"pg-promise": "^11.9.1",
		"reflect-metadata": "^0.2.2",
		"ts-node": "^10.9.2",
		"typeorm": "^0.3.20",
		"typescript": "^5.5.4"
	}
}
